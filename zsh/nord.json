{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [

          {
            "background": "#E5E9F0",
            "foreground": "#4C566A",
            "leading_diamond": "\u256d\u2500\ue0b6",
            "style": "diamond",
            "template":"{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
            "type": "session"
          },
          {
            "background": "#81A1C1",
            "foreground": "#2E3440",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "style": "folder"
            },
            
            "style": "powerline",
            "template": " \uf07b </>{{ .Path }} ",
            "type": "path"
          },
          {
            "background": "#A3BE8C",
            "background_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#D08770{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EBCB8B{{ end }}",
              "{{ if gt .Ahead 0 }}#B48EAD{{ end }}",
              "{{ if gt .Behind 0 }}#B48EAD{{ end }}"
            ],
            "foreground": "#2E3440",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "powerline",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "type": "git"
          },
          {
            "background": "#B48EAD",
            "foreground": "#2E3440",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "always_enabled": true
            },
            "style": "powerline",
            "template": " \ueba2 {{ .FormattedMs }} ",
            "type": "executiontime"
          },
          {
            "background": "#EBCB8B",
            "background_templates": [
              "{{ if gt .Code 0 }}#f1184c{{ end }}"
            ],
            "foreground": "#2E3440",
            "properties": {
              "always_enabled": true
            },
            "style": "diamond",
            "template": " {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e {{ end }}",
            "trailing_diamond": "\ue0b0",
            "type": "status"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "background": "#D08770",
            "foreground": "#2E3440",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "style": "powerline",
            "template": "\uf489 {{ .Name }} </>",
            "type": "shell"
          },
          {
            "background": "#A3BE8C",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "background_templates": [
              "{{if eq \"Charging\" .State.String}}#A3BE8C{{end}}",
              "{{if eq \"Discharging\" .State.String}}#A3BE8C{{end}}",
              "{{if eq \"Full\" .State.String}}#A3BE8C{{end}}"
            ],
            "foreground": "#242424",
            "style": "powerline",
            "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 \ueb2d ",
            "type": "battery"
          },
          {
            "background": "#81A1C1",
            "foreground": "#2E3440",
            "invert_powerline": true,
            "trailing_diamond": "\ue0b4",
            "style": "diamond",
            "template": " \ue641 {{ .CurrentDate | date .Format }} ",
            "type": "time"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#ECEFF4",
            "style": "plain",
            "template": "\u2570\u2500",
            "type": "text"
          },
          {
            "foreground": "#8FBCBB",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#ef5350{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "<#B48EAD>\u276f</><#A3BE8C>\u276f</><#EBCB8B>\u276f</> ",
            "type": "status"
          }
        ],
        "type": "prompt"
      }
    ],
    "version": 2
  }
